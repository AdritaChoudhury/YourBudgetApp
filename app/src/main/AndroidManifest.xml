<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yourbudget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewListActivity"></activity>
        <activity android:name=".ListDatabase" />
        <activity android:name=".DBcontroller" />
        <activity android:name=".DBhelper" />
        <activity android:name=".AddItemsActivity" />
        <activity android:name=".ToolsActivity" />
        <activity android:name=".CreateActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>